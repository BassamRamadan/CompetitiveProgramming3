#include <iostream>
#include <string>

using namespace std;

int main()
{
    string str1, str2;

    cin>>str1>>str2;

    int s1= str1.size(), s2= str2.size();

    int x= (s1 > s2) ? s1 : s2 ;

    if(s1 != s2){
        cout<< x <<endl;
    }
    else if(str1 == str2){
        cout<< "-1"<<endl;
    }
    else{
        cout<< str1.size() <<endl;
    }

    return 0;
}
